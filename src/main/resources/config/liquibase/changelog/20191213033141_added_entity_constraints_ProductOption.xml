<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ProductOption.
    -->
    <changeSet id="20191213033141-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_option_set_id"
                                 baseTableName="product_option"
                                 constraintName="fk_product_option_product_option_set_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_option_set"/>

        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="product_option"
                                 constraintName="fk_product_option_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suppliers"/>

    </changeSet>
</databaseChangeLog>
